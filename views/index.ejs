    <script src="/js/vanta.topology.min.js"></script>
    <script>
        window.addEventListener('resize', animie)
        function animie(){
            effect = VANTA.TOPOLOGY({
                el: '#background',
                mouseControls: true,
                touchControls: true,
                minHeight: 200.00,
                minWidth: 200.00,
                scale: 1.00,
                scaleMobile: 1.00
            })
            effect.destroy();
        }
        animie();
    </script>
